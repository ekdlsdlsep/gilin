<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:backgroundTint="@color/white"
    tools:context=".MainActivity">

    <LinearLayout
        android:id="@+id/mypage_linearLayout1"
        android:layout_width="409dp"
        android:layout_height="100dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="410dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mypage_linearLayout2">

        <androidx.cardview.widget.CardView
            android:layout_width="350dp"
            android:layout_height="55dp"
            android:translationX="29.5dp"
            android:translationY="37.5dp"
            app:cardBackgroundColor="#FAF1D3">

            <TextView
                android:id="@+id/mypage_TextView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="걸음 수"
                android:textColor="#4E4E4E"
                android:textSize="22sp"
                android:translationX="10dp"
                android:translationY="10dp" />

            <TextView
                android:id="@+id/walkCount_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="10,000"
                android:textColor="#000000"
                android:textSize="30sp"
                android:textStyle="bold"
                android:translationX="205dp"
                android:translationY="7dp" />

            <TextView
                android:id="@+id/mypage_TextView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="걸음"
                android:textColor="#272727"
                android:textSize="18sp"
                android:translationX="308dp"
                android:translationY="23dp" />
        </androidx.cardview.widget.CardView>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/mypage_linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/mypage_linearLayout1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mypage_linearLayout3">

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:cardBackgroundColor="#E8CE76"
            app:cardCornerRadius="0dp"
            app:cardElevation="0dp">

            <ImageView
                android:id="@+id/mypage_profileImg"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:background="@drawable/circle"
                android:translationX="20dp"
                android:translationY="20dp"
                android:visibility="visible"
                app:srcCompat="@drawable/ic_launcher_foreground"
                tools:srcCompat="@drawable/ic_launcher_foreground" />

            <TextView
                android:id="@+id/mypage_nickname"
                android:layout_width="114dp"
                android:layout_height="52dp"
                android:text="홍길동"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="36sp"
                android:textStyle="bold"
                android:translationX="112dp"
                android:translationY="13dp" />

            <Button
                android:id="@+id/mypage_button1"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:background="#00ff0000"
                android:text="내 정보 수정"
                android:textColor="#5E5E5E"
                android:textColorLink="#00FFFFFF"
                android:translationX="110dp"
                android:translationY="63dp"
                app:iconTint="#00FFFFFF"
                app:rippleColor="#00FFFFFF" />

            <TextView
                android:id="@+id/mypage_TextView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="즐겨찾기"
                android:textColor="#4E4E4E"
                android:textSize="20sp"
                android:translationX="34dp"
                android:translationY="110dp" />

            <TextView
                android:id="@+id/mypage_TextView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="친구"
                android:textColor="#4E4E4E"
                android:textSize="20sp"
                android:translationX="170dp"
                android:translationY="110dp" />

            <TextView
                android:id="@+id/mypage_TextView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="리뷰"
                android:textColor="#4E4E4E"
                android:textSize="20sp"
                android:translationX="285dp"
                android:translationY="110dp" />

            <EditText
                android:id="@+id/mypage_bookmark"
                android:layout_width="19dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="number"
                android:text="3"
                android:textAlignment="center"
                android:textColor="#4E4E4E"
                android:textSize="20sp"
                android:textStyle="bold"
                android:translationX="113dp"
                android:translationY="101dp" />

            <EditText
                android:id="@+id/mypage_friend"
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="number"
                android:text="11"
                android:textAlignment="center"
                android:textColor="#4E4E4E"
                android:textSize="20sp"
                android:textStyle="bold"
                android:translationX="212dp"
                android:translationY="101dp" />

            <EditText
                android:id="@+id/mypage_review"
                android:layout_width="19dp"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="number"
                android:text="5"
                android:textAlignment="center"
                android:textColor="#4E4E4E"
                android:textSize="20sp"
                android:textStyle="bold"
                android:translationX="326dp"
                android:translationY="101dp" />
        </androidx.cardview.widget.CardView>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/mypage_linearLayout3"
        android:layout_width="409dp"
        android:layout_height="60dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginTop="90dp"
        android:layout_marginBottom="1dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/mypage_linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/mypage_TextView6"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:text="MY"
            android:textAlignment="center"
            android:textSize="24sp"
            android:translationX="175dp"
            android:translationY="20dp" />

        <ImageButton
            android:id="@+id/mypage_imageButton1"
            android:layout_width="20dp"
            android:layout_height="30dp"
            android:backgroundTint="#00FFFFFF"
            android:rotation="-90"
            android:scaleX="1.2"
            android:scaleY="1.2"
            android:translationX="30dp"
            android:translationY="-7dp"
            app:srcCompat="@android:drawable/arrow_up_float" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/mypage_linearLayout4"
        android:layout_width="409dp"
        android:layout_height="320dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="88dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mypage_linearLayout1">

        <android.widget.Button
            android:id="@+id/mypage_button2"
            android:layout_width="350dp"
            android:layout_height="55dp"
            android:background="#FAF1D3"
            android:text="  공지사항"
            android:textAlignment="viewStart"
            android:textColor="#4E4E4E"
            android:textSize="20sp"
            android:translationX="29.5dp"
            android:translationY="22.5dp" />

        <android.widget.Button
            android:id="@+id/mypage_button3"
            android:layout_width="350dp"
            android:layout_height="55dp"
            android:background="#FAF1D3"
            android:text="  설정"
            android:textAlignment="viewStart"
            android:textColor="#4E4E4E"
            android:textSize="20sp"
            android:translationX="29.5dp"
            android:translationY="22.5dp" />

        <android.widget.Button
            android:id="@+id/mypage_button4"
            android:layout_width="350dp"
            android:layout_height="55dp"
            android:background="#FAF1D3"
            android:text="  Q n A"
            android:textAlignment="viewStart"
            android:textColor="#4E4E4E"
            android:textSize="20sp"
            android:translationX="29.5dp"
            android:translationY="22.5dp" />

        <android.widget.Button
            android:id="@+id/mypage_button6"
            android:layout_width="350dp"
            android:layout_height="55dp"
            android:background="#FAF1D3"
            android:text="  고객센터"
            android:textAlignment="viewStart"
            android:textColor="#4E4E4E"
            android:textSize="20sp"
            android:translationX="29.5dp"
            android:translationY="22.5dp" />


    </LinearLayout>

    <LinearLayout

        android:layout_width="409dp"
        android:layout_height="86dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mypage_linearLayout4"/>

</androidx.constraintlayout.widget.ConstraintLayout>